{
  "name": "mediahub",
  "version": "1.0.0",
  "description": "An app where you can combine all your favourie streaming sites and create playlists and offline access.",
  "main": "index.js",
  "scripts": {
    "react:dev": "react-scripts start",
    "electron:watch": "cd src/electron && tsc --watch",
    "electron:dev": "wait-on tcp:127.0.0.1:3000 && electronmon src/electron/dist/main.js",
    "dev": "concurrently \"npm run react:dev\" \"npm run electron:watch\" \"npm run electron:dev\""
  },
  "author": "Carltone Kapfunde",
  "license": "MIT",
  "homepage": ".",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "bootstrap": "^5.3.1",
    "electron": "^26.1.0",
    "ffmpeg-static": "^5.2.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-toolkit": "^1.1.2",
    "sass": "^1.66.1",
    "web-vitals": "^3.4.0",
    "ytdl-core": "^4.11.5",
    "ytsr": "^3.8.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "electron-is-dev": "^2.0.0",
    "electronmon": "^2.0.2",
    "wait-on": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
